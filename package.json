    "prepublishOnly": "pnpm build",
    "release": "release-it"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@squoosh/cli": "^0.7.0",
    "sharp": "^0.33.2"
